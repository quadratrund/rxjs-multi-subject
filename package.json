  "description": "an RxJS special subject, that allows to send differnt events to every subscriber",
  "main": "index.js",
  "scripts": {
    "build": "rm -r build ; tsc && tsc -p tsconfig.esm.json && cp package.json README.md build",
    "tsc": "tsc"
  },
  "author": "Quadratrund",
  "license": "MIT",
  "homepage": "https://github.com/quadratrund/rxjs-multi-subject",
  "repository": {
    "type": "git",
    "url": "https://github.com/quadratrund/rxjs-multi-subject.git"
  },
  "keywords": ["RxJS", "Observables"],
  "dependencies": {
    "rxjs": ">=6.0.0"
  },
  "devDependencies": {
    "typescript": "^5.4.3"
  },
  "types": "./dist/cjs/multi-subject.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/multi-subject.d.ts",
      "node": "./dist/cjs/multi-subject.js",
